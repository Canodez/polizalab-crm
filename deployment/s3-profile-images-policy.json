{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowLambdaToGeneratePresignedUrls",
      "Effect": "Allow",
      "Principal": {
        "Service": "lambda.amazonaws.com"
      },
      "Action": [
        "s3:PutObject",
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::polizalab-profile-images/*"
    }
  ]
}
